<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.slzhkj.smartworksite.model.mapper.LoginMapper" >
    <resultMap id="BaseResultMap" type="com.slzhkj.smartworksite.model.dto.LoginDto">
    </resultMap>

    <sql id="Base_Column_List" >
       id, user_name, password
    </sql>

    <select id="selectAll" resultType="com.slzhkj.smartworksite.model.dto.LoginDto" >
        select
        <include refid="Base_Column_List" />
        from sws_sys_user where is_del = 1
    </select>


</mapper>